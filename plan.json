{
  "version": 1,
  "ops": [
    {
      "id": "frame_id",
      "op": "AGENTYOLO.convert_elapsed_time_to_frame_id",
      "deps": [],
      "args": {
        "seconds": 50.0
      }
    },
    {
      "id": "summary_text",
      "op": "AGENTLLAVA.summarize_video_at_elapsed_time",
      "deps": [],
      "args": {
        "et": 50
      }
    },
    {
      "id": "approx_coords",
      "op": "AGENTKLVR.get_approximate_lat_lon_at_elapsed_time",
      "deps": [],
      "args": {
        "et": 50
      }
    },
    {
      "id": "lat",
      "op": "AGENTKLVR.get_lat",
      "deps": [
        "approx_coords"
      ],
      "args": {}
    },
    {
      "id": "lon",
      "op": "AGENTKLVR.get_lon",
      "deps": [
        "approx_coords"
      ],
      "args": {}
    },
    {
      "id": "freeway_name",
      "op": "AGENTROADY.get_freeway_details_after_elapsed_time",
      "deps": [],
      "args": {
        "et": 50
      }
    },
    {
      "id": "lane_count",
      "op": "AGENTROADY.get_number_of_lanes_after_elapsed_time",
      "deps": [],
      "args": {
        "et": 50
      }
    },
    {
      "id": "final_summary",
      "op": "TEXT.format",
      "deps": [
        "summary_text",
        "lat",
        "lon",
        "freeway_name",
        "lane_count"
      ],
      "args": {
        "template": "At 50 seconds into the video, {0} The vehicle is at coordinates {1}, {2} on the {3}. There are {4} lanes."
      }
    }
  ],
  "outputs": [
    {
      "from": "final_summary",
      "as": "return"
    }
  ]
}